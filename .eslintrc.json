{
	"extends": [
		"eslint:recommended",
		"airbnb",
		"airbnb/hooks",
		"next/core-web-vitals",
		"plugin:prettier/recommended"
	],
	"rules": {
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"react/function-component-definition": [
			2,
			{
				"namedComponents": "function-declaration"
			}
		]
	}
}
